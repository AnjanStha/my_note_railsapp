<h1>Edit Note</h1>

<%= form_with(model: @note, url: note_update_path(@note), method: :patch) do |form| %>
    <%= form.hidden_field :id %>

  
    <%= form.label :title %>
    <%= form.text_field :title %>
  
    <%= form.label :content %>
    <%= form.text_area :content %>
  
    <%= form.submit %>
  <% end %>
  